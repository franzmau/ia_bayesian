
public class Circle implements Person {
	String size;
	int x;
	int y;
	int radius;
	public Circle(String size) {
		// TODO Auto-generated constructor stub
	this.size=size;
	}
	public Circle(){
		
	}

	public void draw(int i) {
		// TODO Auto-generated method stub
		System.out.println("You need "+(i*3.1416)+" of space (like square meters) to draw this circle\n ");
	}

	public void draw(int i, int j) {
		// TODO Auto-generated method stub
		draw(i);
	}

	public void setX(int x) {
		// TODO Auto-generated method stub
		this.x=x;
	}

	
	
	public int getX() {
		// TODO Auto-generated method stub
		return this.x;
	}
	
	  

}
