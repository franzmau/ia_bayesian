public class PersonMaker {
   private Person employee;
   private Person entreperneur;
   private Person square;
   //this is facade class
   public PersonMaker() {
      circle = new Employee();
      rectangle = new Entreperneur();
      square = new Freelance();
   }

   public void drawCircle(int i){
      circle.draw(i);
   }
   public void drawRectangle(int i,int j){
      rectangle.draw(i,j);
   }
   public void drawSquare(int i){
      square.draw(i);
   }
}