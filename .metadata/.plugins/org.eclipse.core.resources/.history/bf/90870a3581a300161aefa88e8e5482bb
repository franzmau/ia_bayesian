
public class game {

	int Board[][];
	final int length=6;
	final int wide= 7;
	final int cpu=2;
	final int player=1;
	
	public game(){
		Board= new int [length][wide];
	}
	public void start_game(){
		for (int i=0; i< length;i++){
			for(int j=0;j<wide;j++){
				Board[i][j]=0;
				System.out.print("[0] ");
				
			}
			
			System.out.print("\n");
		}
	
	}
	
	public Boolean game_finishe(){
		
	}
}
