
public class Employee implements Person {
	String size;
	int x;
	int y;
	int radius;
	public Employee(String size) {
		// TODO Auto-generated constructor stub
	this.size=size;
	}
	public Employee(){
		
	}

	public void income(int i) {
		// TODO Auto-generated method stub
		System.out.println("You need "+(i*3.1416)+" of space (like square meters) to draw this circle\n ");
	}

	public void incom(int i, int j) {
		// TODO Auto-generated method stub
		income(i);
	}

	public void setX(int x) {
		// TODO Auto-generated method stub
		this.x=x;
	}

	
	
	public int getX() {
		// TODO Auto-generated method stub
		return this.x;
	}
	
	  

}
