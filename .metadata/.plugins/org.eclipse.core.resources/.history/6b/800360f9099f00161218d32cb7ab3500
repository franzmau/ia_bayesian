
public class Rectangle implements Person {

	public void draw(int i,int v) {
		// TODO Auto-generated method stub
		
		System.out.println("You need "+(i*v)+" of space (like square meters) to draw this rectangle\n ");

	}

	public void draw(int i) {
		// TODO Auto-generated method stub
		draw(i,i*2);
	}

}
